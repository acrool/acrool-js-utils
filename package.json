{
  "name": "bear-jsutils",
  "version": "3.0.0",
  "description": "Common javascript utils methods for project development",
  "keywords": [
    "typescript",
    "js-utils"
  ],
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imagine10255/bear-jsutils.git"
  },
  "exports": {
    "./array": {
      "require": "./dist/array/index.es.js",
      "import": "./src/array/index.ts",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "require": "./dist/browser/index.es.js",
      "import": "./src/browser/index.ts",
      "types": "./dist/browser.d.ts"
    },
    "./convert": {
      "require": "./dist/convert/index.es.js",
      "import": "./src/convert/index.ts",
      "types": "./dist/convert.d.ts"
    },
    "./date": {
      "require": "./dist/date/index.es.js",
      "import": "./src/date/index.ts",
      "types": "./dist/date.d.ts"
    },
    "./dom": {
      "require": "./dist/dom/index.es.js",
      "import": "./src/dom/index.ts",
      "types": "./dist/dom.d.ts"
    },
    "./enum": {
      "require": "./dist/enum/index.es.js",
      "import": "./src/enum/index.ts",
      "types": "./dist/enum.d.ts"
    },
    "./equal": {
      "require": "./dist/equal/index.es.js",
      "import": "./src/equal/index.ts",
      "types": "./dist/equal.d.ts"
    },
    "./file": {
      "require": "./dist/file/index.es.js",
      "import": "./src/file/index.ts",
      "types": "./dist/file.d.ts"
    },
    "./number": {
      "require": "./dist/number/index.es.js",
      "import": "./src/number/index.ts",
      "types": "./dist/number.d.ts"
    },
    "./object": {
      "require": "./dist/object/index.es.js",
      "import": "./src/object/index.ts",
      "types": "./dist/object.d.ts"
    },
    "./string": {
      "require": "./dist/string/index.es.js",
      "import": "./src/string/index.ts",
      "types": "./dist/string.d.ts"
    },
    "./uri": {
      "require": "./dist/uri/index.es.js",
      "import": "./src/uri/index.ts",
      "types": "./dist/uri.d.ts"
    },
    "./regPattern": {
      "require": "./dist/regPattern/index.es.js",
      "import": "./regPattern/index.ts",
      "types": "./dist/regPattern.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite build -w",
    "build:claer": "rm -rf ./dist",
    "prepublishOnly": "run-s build",
    "build": "run-s build:claer && tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --fix",
    "cz": "git-cz",
    "test": "jest",
    "gitlog": "git log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cblueby %an %Cgreen(%cr)%Creset'",
    "release": "standard-version --release-as",
    "release:major": "standard-version -r major",
    "release:minor": "standard-version -r minor",
    "release:patch": "standard-version -r patch",
    "release:alpha": "standard-version --prerelease alpha"
  },
  "dependencies": {
    "dayjs": "^1.x"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cz-git": "^1.6.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
