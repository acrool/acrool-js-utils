{
  "name": "imagine-js-utils-example",
  "homepage": ".",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint:fix": "eslint ./src --fix",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/node": "link:../node_modules/@types/node",
    "typescript": "link:../node_modules/typescript",
    "@types/deep-equal": "1.0.1",
    "imagine-js-utils": "link:../dist",
    "deep-equal": "^2.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "link:../node_modules/@testing-library/jest-dom",
    "@testing-library/user-event": "link:../node_modules/@testing-library/user-event",
    "@types/jest": "link:../node_modules/@types/jest",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "jest": "27.4.7",
    "ts-jest": "27.1.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
  "rules": {
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "always"
    ],
    "indent": [
      "warn",
      4
    ],
    "object-curly-spacing": [
      "warn",
      "never"
    ],
    "no-useless-escape": "off",
    "import/no-anonymous-default-export": "off",
    "react-hooks/exhaustive-deps": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
}
